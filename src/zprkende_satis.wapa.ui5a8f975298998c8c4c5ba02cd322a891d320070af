sap.ui.define([                                                                                                                                                                                                                                                
    "sap/ui/model/json/JSONModel",                                                                                                                                                                                                                             
    "sap/ui/Device"                                                                                                                                                                                                                                            
],&nbsp;                                                                                                                                                                                                                                                       
    /**                                                                                                                                                                                                                                                        
     * provide app-view type models (as in the first "V" in MVVC)                                                                                                                                                                                              
     *&nbsp;                                                                                                                                                                                                                                                   
     * @param {typeof sap.ui.model.json.JSONModel} JSONModel                                                                                                                                                                                                   
     * @param {typeof sap.ui.Device} Device                                                                                                                                                                                                                    
     *&nbsp;                                                                                                                                                                                                                                                   
     * @returns {Function} createDeviceModel() for providing runtime info for the device the UI5 app is running on                                                                                                                                             
     */                                                                                                                                                                                                                                                        
    function (JSONModel, Device) {                                                                                                                                                                                                                             
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return {                                                                                                                                                                                                                                               
            createDeviceModel: function () {                                                                                                                                                                                                                   
                var oModel = new JSONModel(Device);                                                                                                                                                                                                            
                oModel.setDefaultBindingMode("OneWay");                                                                                                                                                                                                        
                return oModel;                                                                                                                                                                                                                                 
        },                                                                                                                                                                                                                                                     
        createAppBasketModel: function () {                                                                                                                                                                                                                    
            var Sepet = {                                                                                                                                                                                                                                      
                "sepet": [                                                                                                                                                                                                                                     
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                             
                ]                                                                                                                                                                                                                                              
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                                                                                                                                                                                                                                               
            };                                                                                                                                                                                                                                                 
            var oModel = new JSONModel(Sepet);                                                                                                                                                                                                                 
            return oModel;                                                                                                                                                                                                                                     
        },                                                                                                                                                                                                                                                     
        createAppTreeModel: function () {                                                                                                                                                                                                                      
            var Tree = {                                                                                                                                                                                                                                       
                "tree": [                                                                                                                                                                                                                                      
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                             
                ]                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
            };                                                                                                                                                                                                                                                 
            var oModel = new JSONModel(Tree);                                                                                                                                                                                                                  
            return oModel;                                                                                                                                                                                                                                     
        },                                                                                                                                                                                                                                                     
        createAppBagliModel: function () {                                                                                                                                                                                                                     
            var Bagli = {                                                                                                                                                                                                                                      
                "bagli": [                                                                                                                                                                                                                                     
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                             
                ],                                                                                                                                                                                                                                             
                "count": 0                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
            };                                                                                                                                                                                                                                                 
            var oModel = new JSONModel(Bagli);                                                                                                                                                                                                                 
            return oModel;                                                                                                                                                                                                                                     
        },                                                                                                                                                                                                                                                     
        createMusteriModel: function () {                                                                                                                                                                                                                      
            var Musteri = {                                                                                                                                                                                                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                     
            };                                                                                                                                                                                                                                                 
            var oModel = new JSONModel(Musteri);                                                                                                                                                                                                               
            return oModel;                                                                                                                                                                                                                                     
        },                                                                                                                                                                                                                                                     
        createImageModel: function () {                                                                                                                                                                                                                        
            // var path = "https://github.com/janvaljean/https---github.com-Nuwency-Software-KoleksiyonKasa/blob/sefasbranch/webapp/images/1497524501_koleksiyon_chicago.png";                                                                                 
            // var path2 = "webapp/images/1497524501_koleksiyon_chicago.png";                                                                                                                                                                                  
            var path = {                                                                                                                                                                                                                                       
                "path1": "https://github.com/janvaljean/https---github.com-Nuwency-Software-KoleksiyonKasa/blob/sefasbranch/webapp/images/1497524501_koleksiyon_chicago.png",                                                                                  
                "path2": "webapp/images/1497524501_koleksiyon_chicago.png",                                                                                                                                                                                    
                "path3": "http://kolerpt.koleksiyon.mobilya:8000/sap/bc/ui5_ui5/sap/zprkende_satis/mockdata/image/1497524501_koleksiyon_chicago.png",                                                                                                          
                "path4": "sap/bc/ui5_ui5/sap/zprkende_satis/mockdata/image/",                                                                                                                                                                                  
                "path5": "webapp/images/1497524501_koleksiyon_chicago.png",                                                                                                                                                                                    
                "path6": " webapp/mockdata/images/1497524501_koleksiyon_chicago.png",                                                                                                                                                                          
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                   
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
