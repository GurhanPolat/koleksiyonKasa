<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
    xmlns:f="sap.ui.layout.form"                                                                                                                                                                                                                               
>                                                                                                                                                                                                                                                              
<Dialog title="{i18n>customerInfos}" >                                                                                                                                                                                                                         
    <content>                                                                                                                                                                                                                                                  
    <Table id="tab2"  mode="SingleSelectMaster" items="{MusteriSet>/musteriArray}"  selectionChange="musteriTableSelectionChange" >                                                                                                                            
        <columns>                                                                                                                                                                                                                                              
            <Column>                                                                                                                                                                                                                                           
                <Text text="{i18n>customerNo}" />                                                                                                                                                                                                              
            </Column>                                                                                                                                                                                                                                          
            <Column hAlign="Begin" demandPopin="true" >                                                                                                                                                                                                        
                <Text text="{i18n>customerName}" />                                                                                                                                                                                                            
            </Column>                                                                                                                                                                                                                                          
            <Column hAlign="Begin">                                                                                                                                                                                                                            
                <Text text="{i18n>customerTel}"/>                                                                                                                                                                                                              
            </Column>                                                                                                                                                                                                                                          
            <Column hAlign="Begin">                                                                                                                                                                                                                            
                <Text text="{i18n>customerAdress}"/>                                                                                                                                                                                                           
            </Column>                                                                                                                                                                                                                                          
            <Column hAlign="Begin">                                                                                                                                                                                                                            
                <Text text="{i18n>customerEmail}"/>                                                                                                                                                                                                            
            </Column>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        </columns>                                                                                                                                                                                                                                             
        <items>                                                                                                                                                                                                                                                
            <ColumnListItem>                                                                                                                                                                                                                                   
                <cells>                                                                                                                                                                                                                                        
                    <Text text="{MusteriSet>Kunnr}" textAlign="Begin" />                                                                                                                                                                                       
                    <Text text="{MusteriSet>TamIsim}" textAlign="Begin"/>                                                                                                                                                                                      
                    <Text text="{MusteriSet>TelNumber}" textAlign="Begin"/>                                                                                                                                                                                    
                    <Text text="{MusteriSet>TamAdres}" textAlign="Begin"/>                                                                                                                                                                                     
                    <Text text="{MusteriSet>SmtpAddr}" textAlign="Begin"/>                                                                                                                                                                                     
                </cells>                                                                                                                                                                                                                                       
            </ColumnListItem>                                                                                                                                                                                                                                  
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                             
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                   
        </items>                                                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                     
    </Table>                                                                                                                                                                                                                                                   
&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
    </content>                                                                                                                                                                                                                                                 
    <beginButton>&nbsp;                                                                                                                                                                                                                                        
        <!-- <Button text="MÃ¼Återi Ekle" press="MusteriEklePress" type="Emphasized" class="sapUiTinyMarginBeginEnd"></Button> -->                                                                                                                            
    </beginButton>                                                                                                                                                                                                                                             
    <endButton>                                                                                                                                                                                                                                                
        <Button text="{i18n>close}" press="onCloseButtonPress"></Button>                                                                                                                                                                                       
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
    </endButton>                                                                                                                                                                                                                                               
</Dialog>                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
</c:FragmentDefinition>                                                                                                                                                                                                                                        