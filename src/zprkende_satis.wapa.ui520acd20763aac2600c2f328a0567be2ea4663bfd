    function (Controller, JSONModel, ) {                                                                                                                                                                                                                       
        "use strict";                                                                                                                                                                                                                                          
var svgLogo;                                                                                                                                                                                                                                                   
var svgLogo2                                                                                                                                                                                                                                                   
        return Controller.extend("test.controller.View0", {                                                                                                                                                                                                    
