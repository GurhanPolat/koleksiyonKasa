&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                                 
                if (sServiceUrl.includes("dev")) {                                                                                                                                                                                                             
                  console.log("Development Environment");                                                                                                                                                                                                      
                } else if (sServiceUrl.includes("qa")) {                                                                                                                                                                                                       
                  console.log("QA Environment");                                                                                                                                                                                                               
